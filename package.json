{
  "name": "solana-contracts-backend",
  "version": "2.0.0",
  "description": "API for managing Solana token contracts with wallet authentication",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec \"ts-node src/index.ts\"",
    "dev:nodemon": "nodemon --exec \"ts-node src/index.ts\"",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "start:ts": "ts-node src/index.ts"
  },
  "dependencies": {
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.4",
    "@supabase/supabase-js": "^2.39.0",
    "@types/ws": "^8.18.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "nodemon": "^3.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  }
}
